<script>
    var app = new Vue({
        el: "#vue",
        data: {
            urls: [
                {
                    url: ""
                }
            ]
        },
        methods: {
            addUrl(event) {
                if (event) {
                    event.preventDefault();
                }
                this.urls.push(
                    {url: ""}
                )
            },
            deleteUrl(index, event) {
                if (event) {
                    event.preventDefault();
                }
                this.urls.splice(index, 1);
            }
        }
    });
</script>
