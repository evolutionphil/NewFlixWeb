<head>
    <%
        let title1=typeof title!='undefined' ? title : 'Flix IPTV';
        let keyword1=typeof keyword!='undefined' ? keyword : '' ;
        let description1=typeof description!='undefined' ? description : 'Professional IPTV streaming platform with 7-day free trial. Upload your playlist and enjoy premium streaming experience.' ;
        let image1=typeof ogImage!='undefined' ? ogImage : '/images/logo.png';
        let url1=typeof canonicalUrl!='undefined' ? canonicalUrl : process.env.REPLIT_DEV_DOMAIN ? `https://${process.env.REPLIT_DEV_DOMAIN}/mylist` : 'https://flixapp.net/mylist';
    %>
    <title><%= title1 %></title>
    <meta charset="utf-8">
    <meta name="keywords" content="<%= keyword1 %>">
    <meta name="description" content="<%= description1 %>">
    <link rel="icon" href="/images/logo.png"/>
    
    <!-- Apple Touch Icons for iOS devices -->
    <link rel="apple-touch-icon" href="/images/logo.png">
    <link rel="apple-touch-icon" sizes="57x57" href="/images/logo.png">
    <link rel="apple-touch-icon" sizes="60x60" href="/images/logo.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/images/logo.png">
    <link rel="apple-touch-icon" sizes="76x76" href="/images/logo.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/images/logo.png">
    <link rel="apple-touch-icon" sizes="120x120" href="/images/logo.png">
    <link rel="apple-touch-icon" sizes="144x144" href="/images/logo.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/images/logo.png">
    <link rel="apple-touch-icon" sizes="180x180" href="/images/logo.png">
    
    <!-- Additional iOS/Safari Meta Tags -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
    <meta name="apple-mobile-web-app-title" content="Flix IPTV">
    
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
    <!-- Canonical URL -->
    <link rel="canonical" href="<%= url1 %>">
    
    <!-- Open Graph Meta Tags -->
    <meta property="og:type" content="website">
    <meta property="og:title" content="<%= title1 %>">
    <meta property="og:description" content="<%= description1 %>">
    <meta property="og:image" content="<%= url1.split('/').slice(0,3).join('/') + image1 %>">
    <meta property="og:url" content="<%= url1 %>">
    <meta property="og:site_name" content="Flix IPTV">
    <meta property="og:locale" content="en_US">
    
    <!-- Twitter Card Meta Tags -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:title" content="<%= title1 %>">
    <meta name="twitter:description" content="<%= description1 %>">
    <meta name="twitter:image" content="<%= url1.split('/').slice(0,3).join('/') + image1 %>">
    <meta name="twitter:creator" content="@FlixIPTV">
    
    <!-- Additional SEO Meta Tags -->
    <meta name="robots" content="index, follow">
    <meta name="language" content="en">
    <meta name="author" content="Flix IPTV">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="theme-color" content="#473590">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Plus+Jakarta+Sans:ital,wght@0,200..800;1,200..800&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.2/css/all.min.css"/>
    <link rel="stylesheet" href="/admin/template/css/bootstrap.min.css">
    <link rel="stylesheet" href="/admin/template/fonts/font-awesome/font-awesome.css">

    <script src="/admin/template/vendor/jquery/jquery.js"></script>
    <script src="/admin/template/vendor/popper-js/umd/popper.min.js"></script>
    <script src="/admin/template/vendor/bootstrap/bootstrap.js"></script>

    <link rel="stylesheet" href="/frontend/style.css">
    
    <!-- Schema.org Structured Data -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "WebSite",
        "name": "Flix IPTV",
        "alternateName": "FlixIPTV",
        "url": "<%= url1.split('/').slice(0,3).join('/') %>",
        "description": "<%= description1 %>",
        "publisher": {
            "@type": "Organization",
            "name": "Flix IPTV",
            "logo": {
                "@type": "ImageObject",
                "url": "<%= url1.split('/').slice(0,3).join('/') %>/images/logo.png"
            }
        },
        "potentialAction": {
            "@type": "SearchAction",
            "target": {
                "@type": "EntryPoint",
                "urlTemplate": "<%= url1.split('/').slice(0,3).join('/') %>/news?q={search_term_string}"
            },
            "query-input": "required name=search_term_string"
        },
        "sameAs": [
            "https://twitter.com/FlixIPTV",
            "https://facebook.com/FlixIPTV"
        ]
    }
    </script>
    
    <% if (typeof pageType !== 'undefined' && pageType === 'article') { %>
    <!-- Article Structured Data -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Article",
        "headline": "<%= title1 %>",
        "description": "<%= description1 %>",
        "image": "<%= url1.split('/').slice(0,3).join('/') + image1 %>",
        "author": {
            "@type": "Organization",
            "name": "Flix IPTV"
        },
        "publisher": {
            "@type": "Organization",
            "name": "Flix IPTV",
            "logo": {
                "@type": "ImageObject",
                "url": "<%= url1.split('/').slice(0,3).join('/') %>/images/logo.png"
            }
        },
        "dateModified": "<%= new Date().toISOString() %>",
        "datePublished": "<%= new Date().toISOString() %>",
        "mainEntityOfPage": {
            "@type": "WebPage",
            "@id": "<%= url1 %>"
        }
    }
    </script>
    <% } %>
    
    <% if (typeof pageType !== 'undefined' && pageType === 'service') { %>
    <!-- Service/Product Structured Data -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Product",
        "name": "Flix IPTV Streaming Service",
        "description": "Professional IPTV streaming platform with 7-day free trial. Upload your playlist and enjoy premium streaming experience.",
        "image": "<%= url1.split('/').slice(0,3).join('/') %>/images/logo.png",
        "brand": {
            "@type": "Brand",
            "name": "Flix IPTV"
        },
        "offers": {
            "@type": "Offer",
            "price": "7.49",
            "priceCurrency": "USD",
            "availability": "https://schema.org/InStock",
            "seller": {
                "@type": "Organization",
                "name": "Flix IPTV"
            }
        }
    }
    </script>
    <% } %>
    
    <% if (typeof pageType !== 'undefined' && pageType === 'faq') { %>
    <!-- FAQ Page Structured Data -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "FAQPage",
        "name": "<%= title1 %>",
        "description": "<%= description1 %>",
        "url": "<%= url1 %>",
        "mainEntity": [
            {
                "@type": "Question",
                "name": "How do I start using Flix IPTV?",
                "acceptedAnswer": {
                    "@type": "Answer",
                    "text": "Simply upload your playlist and start your 7-day free trial. After the trial period, you can activate your account to continue using our premium streaming service."
                }
            },
            {
                "@type": "Question", 
                "name": "What devices are supported?",
                "acceptedAnswer": {
                    "@type": "Answer",
                    "text": "Flix IPTV supports Android, iOS, Smart TV, and Windows devices. You can use the same account across multiple devices."
                }
            }
        ],
        "author": {
            "@type": "Organization",
            "name": "Flix IPTV"
        }
    }
    </script>
    <% } %>
    
    <% if (typeof pageType !== 'undefined' && pageType === 'news-listing') { %>
    <!-- News/Blog Listing Page Structured Data -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Blog",
        "name": "<%= title1 %>",
        "description": "<%= description1 %>",
        "url": "<%= url1 %>",
        "publisher": {
            "@type": "Organization",
            "name": "Flix IPTV",
            "logo": {
                "@type": "ImageObject",
                "url": "<%= url1.split('/').slice(0,3).join('/') %>/images/logo.png"
            }
        },
        "author": {
            "@type": "Organization",
            "name": "Flix IPTV"
        },
        "mainEntityOfPage": {
            "@type": "WebPage",
            "@id": "<%= url1 %>"
        }
    }
    </script>
    <% } %>
    
    <% if (typeof pageType !== 'undefined' && pageType === 'youtube-service') { %>
    <!-- YouTube Service Page Structured Data -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "WebApplication",
        "name": "<%= title1 %>",
        "description": "<%= description1 %>",
        "url": "<%= url1 %>",
        "applicationCategory": "MultimediaApplication",
        "operatingSystem": ["Android", "iOS", "Windows", "Smart TV"],
        "offers": {
            "@type": "Offer",
            "price": "0",
            "@id": "<%= url1 %>",
            "priceCurrency": "USD",
            "availability": "https://schema.org/InStock",
            "description": "Free YouTube playlist integration with 7-day trial"
        },
        "publisher": {
            "@type": "Organization",
            "name": "Flix IPTV",
            "logo": {
                "@type": "ImageObject",
                "url": "<%= url1.split('/').slice(0,3).join('/') %>/images/logo.png"
            }
        },
        "featureList": [
            "YouTube Playlist Upload",
            "Multi-device Streaming",
            "Cross-platform Compatibility",
            "Free 7-day Trial"
        ]
    }
    </script>
    <% } %>
    
    <% if (typeof pageType !== 'undefined' && pageType === 'contact') { %>
    <!-- Contact Page Structured Data -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": ["ContactPage", "WebPage"],
        "name": "<%= title1 %>",
        "description": "<%= description1 %>",
        "url": "<%= url1 %>",
        "mainEntity": {
            "@type": "Organization",
            "name": "Flix IPTV",
            "url": "<%= url1.split('/').slice(0,3).join('/') %>",
            "logo": {
                "@type": "ImageObject",
                "url": "<%= url1.split('/').slice(0,3).join('/') %>/images/logo.png"
            },
            "contactPoint": {
                "@type": "ContactPoint",
                "contactType": "customer support",
                "email": "support@evolution-world.com",
                "availableLanguage": ["English"],
                "areaServed": "Worldwide"
            },
            "sameAs": [
                "https://twitter.com/FlixIPTV",
                "https://facebook.com/FlixIPTV"
            ]
        },
        "breadcrumb": {
            "@type": "BreadcrumbList",
            "itemListElement": [
                {
                    "@type": "ListItem",
                    "position": 1,
                    "name": "Home",
                    "item": "<%= url1.split('/').slice(0,3).join('/') %>"
                },
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "Contact",
                    "item": "<%= url1 %>"
                }
            ]
        }
    }
    </script>
    <% } %>
    
    <% if (typeof pageType !== 'undefined' && pageType === 'instruction-guide') { %>
    <!-- Device Instruction Guide Structured Data -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "HowTo",
        "name": "<%= title1 %>",
        "description": "<%= description1 %>",
        "url": "<%= url1 %>",
        "image": {
            "@type": "ImageObject",
            "url": "<%= url1.split('/').slice(0,3).join('/') %>/images/logo.png"
        },
        "author": {
            "@type": "Organization",
            "name": "Flix IPTV",
            "url": "<%= url1.split('/').slice(0,3).join('/') %>"
        },
        "publisher": {
            "@type": "Organization",
            "name": "Flix IPTV",
            "logo": {
                "@type": "ImageObject",
                "url": "<%= url1.split('/').slice(0,3).join('/') %>/images/logo.png"
            }
        },
        "mainEntity": {
            "@type": "SoftwareApplication",
            "name": "Flix IPTV",
            "applicationCategory": "MultimediaApplication",
            "operatingSystem": "<%= typeof deviceType !== 'undefined' ? deviceType.charAt(0).toUpperCase() + deviceType.slice(1) : 'Multiple Platforms' %>",
            "offers": {
                "@type": "Offer",
                "price": "0",
                "priceCurrency": "USD",
                "description": "Free 7-day trial"
            }
        },
        "breadcrumb": {
            "@type": "BreadcrumbList",
            "itemListElement": [
                {
                    "@type": "ListItem",
                    "position": 1,
                    "name": "Home",
                    "item": "<%= url1.split('/').slice(0,3).join('/') %>"
                },
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "Instructions",
                    "item": "<%= url1.split('/').slice(0,4).join('/') %>"
                },
                {
                    "@type": "ListItem",
                    "position": 3,
                    "name": "<%= typeof deviceType !== 'undefined' ? deviceType.charAt(0).toUpperCase() + deviceType.slice(1) : 'Device Setup' %>",
                    "item": "<%= url1 %>"
                }
            ]
        }
    }
    </script>
    <% } %>
    
    <% if (typeof pageType !== 'undefined' && pageType === 'epg-directory') { %>
    <!-- EPG Directory Page Structured Data -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": ["WebPage", "DataCatalog"],
        "name": "<%= title1 %>",
        "description": "<%= description1 %>",
        "url": "<%= url1 %>",
        "mainEntity": {
            "@type": "Dataset",
            "name": "EPG Codes Database",
            "description": "Comprehensive database of Electronic Program Guide codes for streaming channels worldwide with 848+ channel identifiers",
            "keywords": ["EPG", "Electronic Program Guide", "IPTV", "TV Guide", "Channel Codes", "streaming integration", "broadcasting solutions"],
            "publisher": {
                "@type": "Organization",
                "name": "Flix IPTV",
                "url": "<%= url1.split('/').slice(0,3).join('/') %>"
            },
            "distribution": {
                "@type": "DataDownload",
                "contentUrl": "<%= url1 %>",
                "encodingFormat": "text/html"
            },
            "spatialCoverage": "Worldwide",
            "temporalCoverage": "2025",
            "variableMeasured": ["Channel Identifier", "Country Origin", "Content Category", "EPG Code"]
        },
        "publisher": {
            "@type": "Organization",
            "name": "Flix IPTV",
            "logo": {
                "@type": "ImageObject",
                "url": "<%= url1.split('/').slice(0,3).join('/') %>/images/logo.png"
            }
        },
        "breadcrumb": {
            "@type": "BreadcrumbList",
            "itemListElement": [
                {
                    "@type": "ListItem",
                    "position": 1,
                    "name": "Home",
                    "item": "<%= url1.split('/').slice(0,3).join('/') %>"
                },
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "EPG Codes",
                    "item": "<%= url1 %>"
                }
            ]
        }
    }
    </script>
    <% } %>
    
    <% if (typeof pageType !== 'undefined' && pageType === 'country-epg-codes') { %>
    <!-- Country-Specific EPG Codes Page Structured Data -->
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": ["WebPage", "DataCatalog"],
        "name": "<%= title1 %>",
        "description": "<%= description1 %>",
        "url": "<%= url1 %>",
        "mainEntity": {
            "@type": "Dataset",
            "name": "<%= typeof country !== 'undefined' ? country + ' EPG Codes Database' : 'Country EPG Codes Database' %>",
            "description": "<%= typeof country !== 'undefined' ? 'Professional ' + country + ' Electronic Program Guide database with comprehensive TV channel identifiers for streaming integration and broadcasting solutions' : 'Country-specific Electronic Program Guide database' %>",
            "keywords": ["<%= typeof country !== 'undefined' ? country : 'Country' %> EPG", "Electronic Program Guide", "<%= typeof country !== 'undefined' ? country : 'Country' %> TV Channels", "Streaming Codes", "Broadcasting Identifiers"],
            "spatialCoverage": "<%= typeof country !== 'undefined' ? country : 'Specific Country' %>",
            "temporalCoverage": "2025",
            "variableMeasured": ["Channel Identifier", "Content Category", "EPG Code"],
            "publisher": {
                "@type": "Organization",
                "name": "Flix IPTV",
                "url": "<%= url1.split('/').slice(0,3).join('/') %>"
            },
            "distribution": {
                "@type": "DataDownload",
                "contentUrl": "<%= url1 %>",
                "encodingFormat": "text/html"
            }
        },
        "publisher": {
            "@type": "Organization",
            "name": "Flix IPTV",
            "logo": {
                "@type": "ImageObject",
                "url": "<%= url1.split('/').slice(0,3).join('/') %>/images/logo.png"
            }
        },
        "breadcrumb": {
            "@type": "BreadcrumbList",
            "itemListElement": [
                {
                    "@type": "ListItem",
                    "position": 1,
                    "name": "Home",
                    "item": "<%= url1.split('/').slice(0,3).join('/') %>"
                },
                {
                    "@type": "ListItem",
                    "position": 2,
                    "name": "EPG Codes",
                    "item": "<%= url1.split('/').slice(0,-1).join('/') %>"
                },
                {
                    "@type": "ListItem",
                    "position": 3,
                    "name": "<%= typeof country !== 'undefined' ? country + ' Channels' : 'Country Channels' %>",
                    "item": "<%= url1 %>"
                }
            ]
        },
        "isPartOf": {
            "@type": "DataCatalog",
            "name": "EPG Codes Database",
            "url": "<%= url1.split('/').slice(0,-1).join('/') %>"
        }
    }
    </script>
    <% } %>

</head>
